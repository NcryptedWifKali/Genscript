<Window x:Class="HintLabwares.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Labwares needed" Height="370" Width="525">
    <Grid>
        <ListBox Height="219" HorizontalAlignment="Left" Margin="25,48,0,0" Name="lstSrcPlates" VerticalAlignment="Top" Width="203" ItemsSource="{Binding}"/>
        <Label Content="Source Plate Names:" Height="28" HorizontalAlignment="Left" Margin="25,12,0,0" Name="lblSrcPlateNames" VerticalAlignment="Top" Width="203" />
        <ListBox Height="219" HorizontalAlignment="Left" Margin="249,48,0,0" Name="lstDest" VerticalAlignment="Top" Width="203" ItemsSource="{Binding}" />
        <Label Content="Dest 16Pos Tube Carriers:" Height="28" HorizontalAlignment="Left" Margin="249,12,0,0" Name="lblDest" VerticalAlignment="Top" Width="203" />
        <Button Content="Close" Height="46" HorizontalAlignment="Left" Margin="249,273,0,0" Name="btnClose" VerticalAlignment="Top" Width="203" Click="btnClose_Click" />
        <Label Content="" Height="28" HorizontalAlignment="Left" Margin="25,283,0,0" Name="lblTotalCnt" VerticalAlignment="Top" />
    </Grid>
</Window>
